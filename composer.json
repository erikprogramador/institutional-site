{
    "name": "erikprogramador/institutional-site",
    "description": "A institutional site made with silex, twig and doctrine",
    "require": {
        "silex/silex": "^2.0",
        "twig/twig": "^2.3",
        "symfony/twig-bridge": "^3.2",
        "dflydev/doctrine-orm-service-provider": "^2.0",
        "doctrine/dbal": "^2.5",
        "doctrine/common": "^2.7",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "authors": [
        {
            "name": "Erik Vanderlei Fernandes",
            "email": "erik.vanderlei.programador@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/utils.php"]
    },
    "config": {
        "bin-dir": "bin"
    }
}
